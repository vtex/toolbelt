{
  "extends": "vtex",
  "root": true,
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-console": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "_+" }],
    "@typescript-eslint/explicit-member-accessibility": "off"
  }
}
