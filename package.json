{
  "name": "vtex",
  "description": "The platform for e-commerce apps",
  "version": "2.97.1-beta.1",
  "bin": "./bin/run",
  "bugs": "https://github.com/vtex/toolbelt/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@tiagonapoli/oclif-plugin-spaced-commands": "^0.0.2",
    "@vtex/api": "3.71.1",
    "@yarnpkg/lockfile": "^1.1.0",
    "ajv": "~6.10.2",
    "any-promise": "^1.3.0",
    "archiver": "^3.1.1",
    "async-retry": "~1.2.3",
    "bluebird": "^3.5.0",
    "boxen": "^4.2.0",
    "chalk": "~2.3.2",
    "child-process-es6-promise": "~1.2.1",
    "chokidar": "~3.3.0",
    "clear-module": "~4.0.0",
    "cli-table": "~0.3.1",
    "cli-table2": "~0.2.0",
    "clipboardy": "~2.1.0",
    "configstore": "^5.0.1",
    "csvtojson": "~2.0.10",
    "debounce": "~1.2.0",
    "diff": "~3.5.0",
    "emojic": "^1.1.15",
    "enquirer": "~2.3.2",
    "eventsource": "~1.0.7",
    "extendable-error": "~0.1.5",
    "findhelp": "~1.1.0",
    "fs-extra": "~7.0.0",
    "get-stream": "~4.0.0",
    "globby": "~8.0.1",
    "ink": "^2.7.1",
    "is-docker": "^2.0.0",
    "is-wsl": "^2.1.1",
    "js-yaml": "~3.13.1",
    "json-array-split": "~1.0.0",
    "json2csv": "~4.5.4",
    "jsonwebtoken": "~8.5.1",
    "latest-version": "^4.0.0",
    "lint": "^0.7.0",
    "lockfile": "^1.0.4",
    "moment": "~2.24.0",
    "node-notifier": "^6.0.0",
    "numbro": "2.1.0",
    "open": "^7.0.2",
    "opn": "~5.2.0",
    "ora": "^4.0.3",
    "pad": "~2.0.3",
    "pipe-streams-to-promise": "~0.2.0",
    "progress": "~2.0.3",
    "prompt-confirm": "^2.0.4",
    "prompts": "~2.3.0",
    "qrcode-terminal": "~0.12.0",
    "ramda": "~0.25.0",
    "ramda-adjunct": "~2.23.0",
    "randomstring": "~1.1.5",
    "react": "^16.11.0",
    "request": "~2.88.0",
    "semver": "~7.1.1",
    "semver-diff": "~2.1.0",
    "tar": "~4.4.10",
    "tslib": "^1",
    "unzip-stream": "~0.3.0",
    "update-notifier": "~3.0.1",
    "v8-compile-cache": "^2.1.0",
    "winston": "~3.2.1",
    "winston-transport": "~4.3.0",
    "ws": "^7.2.3",
    "yarn": "~1.21.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/async-retry": "1.4.1",
    "@types/chai": "^4",
    "@types/configstore": "^4.0.0",
    "@types/debounce": "^1.2.0",
    "@types/fs-extra": "5.0.4",
    "@types/jest": "24.0.23",
    "@types/jsonwebtoken": "8.3.5",
    "@types/mocha": "^5",
    "@types/node": "10.12.12",
    "@types/node-notifier": "^5.4.0",
    "@types/prompts": "^2.0.3",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/randomstring": "^1.1.6",
    "@types/react": "^16.9.23",
    "@types/semver-diff": "2.1.1",
    "@types/tar": "4.0.3",
    "@types/yarnpkg__lockfile": "^1.1.3",
    "chai": "^4",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-vtex": "^12.3.1",
    "eslint-config-vtex-react": "^6.3.1",
    "globby": "^10",
    "jest": "24.9.0",
    "mocha": "^5",
    "nodemon": "^2.0.2",
    "nodemon-notifier-cli": "https://github.com/Slessi/nodemon-notifier-cli.git",
    "nyc": "^14",
    "prettier": "^1.19.1",
    "ts-jest": "24.2.0",
    "ts-node": "^8",
    "typescript": "^3.8.2"
  },
  "engines": {
    "node": ">=10"
  },
  "engineStrict": true,
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "scripts/closeChrome.scpt",
    "lib/"
  ],
  "homepage": "https://github.com/vtex/toolbelt",
  "keywords": [
    "CLI",
    "vtex",
    "ecommerce",
    "commerce",
    "omnichannel",
    "platform",
    "react",
    "toolbelt",
    "apps",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vtex",
    "plugins": [
      "@oclif/plugin-help",
      "@tiagonapoli/oclif-plugin-spaced-commands"
    ],
    "hooks": {
      "init": "./lib/oclif/hooks/init"
    },
    "topics": {
      "config": { "description": "Config commands. Run 'vtex config --help' to see all subcommands" },
      "debug": { "description": "Debug commands. Run 'vtex debug --help' to see all subcommands" },
      "deps": { "description": "Deps commands. Run 'vtex deps --help' to see all subcommands" },
      "edition": { "description": "Edition commands. Run 'vtex edition --help' to see all subcommands" },
      "infra": { "description": "Infra commands. Run 'vtex infra --help' to see all subcommands" },
      "local": { "description": "Local commands. Run 'vtex local --help' to see all subcommands" },
      "redirects": { "description": "Redirects commands. Run 'vtex redirects --help' to see all subcommands" },
      "settings": { "description": "Settings commands. Run 'vtex settings --help' to see all subcommands" },
      "test": { "description": "Test commands. Run 'vtex test --help' to see all subcommands" },
      "workspace": { "description": "Workspace commands. Run 'vtex workspace --help' to see all subcommands" }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vtex/toolbelt.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && OCLIF_COMPILATION=true oclif-dev manifest && OCLIF_COMPILATION=true oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "exe": "node scripts/make-executable.js",
    "watch": "bash ./scripts/symlink.sh && yarn nodemon && yarn oclif-dev manifest",
    "format": "prettier --config ./.prettierrc --write \"./src/**/*.{ts,tsx,js,jsx,json}\"",
    "lint:node": "eslint ./src --cache --ext ts --config .eslintrc",
    "lint:react": "eslint ./src --cache --cache-location .eslintcache-react --ext tsx --config .eslintrc-react",
    "format-lint": "yarn format && yarn lint:node && yarn lint:react",
    "build-clean": "rm -rf lib",
    "build-incremental": "tsc --incremental && OCLIF_COMPILATION=true yarn oclif-dev manifest",
    "build": "yarn build-clean && tsc && OCLIF_COMPILATION=true yarn oclif-dev manifest",
    "test": "jest",
    "prepublishOnly": "bash ./scripts/publishLock.sh",
    "ci:test": "yarn test --ci",
    "ci:prettier-check": "prettier --check --config ./.prettierrc \"./src/**/*.{ts,tsx,js,jsx,json}\""
  },
  "types": "lib/index.d.ts"
}
